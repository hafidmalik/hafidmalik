#include<iostream>
using namespace std;
int main()
{
   char c;
	
	cout<<"Enter a character : ";
	cin>>c;
	
	if(c>=65 && c<=90)
		cout<<"\n\nYou've entered a capital letter.";
	else if(c>=48 && c<=57)
		cout<<"\n\nYou've entered a number.";
	else if(c>=97 && c<=122)
		cout<<"\n\nYou've entered a small letter.";
	else
		cout<<"\n\nYou've entered a special symbol.";
	return 0;	
    
}
