#include <iostream>
using namespace std;

int main()
{
	
int x1,x2,x3,y1,y2,y3,ar;
	
	cout<<"Enter x-y coordinates of first point : ";
	cin>>x1>>y1;
	
	cout<<"\nEnter x-y coordinates of second point : ";
	cin>>x2>>y2;
	
	cout<<"\nEnter x-y coordinates of third point : ";
	cin>>x3>>y3;
	
	ar= (x1*(y2-y3) + x2*(y3-y1) + x3*(y1-y2));//condition for collinear
	
	if(!ar)
		cout<<"\n\nThe points are collinear.";
	else
		cout<<"\n\nThe points are not collinear.";
		
}
